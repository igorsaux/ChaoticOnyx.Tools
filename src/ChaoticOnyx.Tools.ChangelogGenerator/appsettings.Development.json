{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information",
            "ChaoticOnyx.ChangelogGenerator": "Trace"
        }
    },
    "Options": {
        "ChangelogsFolder": "samples/",
        "ChangelogCache": "samples/.all_changelog.yml",
        "OutputChangelog": "samples/changelog.html",
        "DryRun": false,
        "AutoConvert": true,
        "DateCulture": "ru"
    }
}
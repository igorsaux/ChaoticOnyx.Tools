{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information",
            "ChaoticOnyx.ChangelogGenerator": "Information"
        }
    },
    "Options": {
        "ChangelogsFolder": "../../html/changelogs/",
        "ChangelogCache": "../../html/changelogs/.all_changelog.yml",
        "OutputChangelog": "../../html/changelog.html",
        "ChangelogDateFormat": "dd MMMM yyy",
        "DryRun": false,
        "AutoConvert": true,
        "GenerateHTML": true,
        "DateCulture": "ru",
        "Template": "./templates/chaoticonyx.tmpl",
        "ValidPrefixes": [
            "bugfix",
            "wip",
            "tweak",
            "soundadd",
            "sounddel",
            "rscdel",
            "rscadd",
            "imageadd",
            "imagedel",
            "maptweak",
            "spellcheck",
            "experiment",
            "balance",
            "admin"
        ]
    }

}